{
  "name": "gfycat-auto-uploader",
  "version": "0.2.0",
  "author": "Cameron Warner",
  "keywords": [
    "gfycat",
    "video",
    "upload",
    "gif"
  ],
  "description": "Automatically uploads videos from watched folders to Gfycat.com",
  "license": "GPL-3.0",
  "main": "src/main.ts",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "tslint src",
    "postinstall": "./node_modules/.bin/electron-rebuild"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "gfycat_auto_uploader"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "gfycatautouploader"
      }
    }
  },
  "dependencies": {
    "electron": "^2.0.1",
    "electron-compile": "^6.4.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-store": "^1.3.0",
    "electron-titlebar": "0.0.3",
    "form-data": "^2.3.2",
    "keytar": "^4.2.1",
    "lodash": "^4.17.10",
    "react": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.6.2",
    "react-hot-loader": "^4.2.0",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "serve-favicon": "^2.5.0",
    "strongly-typed-events": "^1.4.6",
    "tslib": "^1.9.1",
    "typed-rest-client": "^0.10.0",
    "watchr": "^3.0.1"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.0.3",
    "@types/electron-store": "^1.3.0",
    "@types/form-data": "^2.2.1",
    "@types/keytar": "^4.0.1",
    "@types/lodash": "^4.14.109",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.1.2",
    "@types/node-persist": "0.0.32",
    "@types/react": "^16.3.14",
    "@types/react-bootstrap": "^0.31.9",
    "@types/react-dom": "^16.0.5",
    "@types/react-router": "^4.0.25",
    "@types/react-router-bootstrap": "^0.24.4",
    "@types/react-router-dom": "^4.2.6",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.0",
    "electron-rebuild": "^1.7.3",
    "mocha": "^5.2.0",
    "run-sequence": "^1.2.2",
    "tsc-watch": "^1.0.21",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
