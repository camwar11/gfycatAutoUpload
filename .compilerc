{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": 1.7
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-async-to-generator", "transform-es2015-classes", "react-hot-loader/babel"
        ],
        "sourceMaps": "inline"
      },
      "text/typescript": {
        "sourceMap": "true",
        "inlineSourceMap": "true",
        "inlineSources": "true"
      },
      "text/tsx": {
        "jsx": "react"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": 1.7
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-async-to-generator", "transform-es2015-classes"
        ],
        "sourceMaps": "none"
      },
      "text/typescript": {
        "sourceMap": "false"
      },
      "text/tsx": {
        "jsx": "react"
      }
    }
  }
}