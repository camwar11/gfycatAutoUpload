{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": 1.7
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-async-to-generator", "transform-es2015-classes", "react-hot-loader/babel"
        ],
        "sourceMaps": "inline"
      },
      "text/typescript": {
        "inlineSourceMap": true,
        "inlineSources": true,
        "jsx": "react"
      },
      "text/tsx": {
        "inlineSourceMap": true,
        "inlineSources": true,
        "jsx": "react"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": 1.7
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-async-to-generator", "transform-es2015-classes"
        ],
        "sourceMaps": "none"
      },
      "text/typescript": {
        "removeComments": false,
        "preserveConstEnums": true,
        "declaration": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "suppressImplicitAnyIndexErrors": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noImplicitThis": true,
        "noUnusedParameters": true,
        "sourceMap": false,
        "importHelpers": true,
        "noEmitHelpers": true,
        "experimentalDecorators": true,
        "target": "es2015",
        "jsx": "react"
      },
      "text/tsx": {
        "jsx": "react"
      }
    }
  }
}